{
	"compilerOptions": {
		"target": "ES6",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"noEmit": true,
		"forceConsistentCasingInFileNames": true,
		"incremental": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"noImplicitAny": true,
		"noImplicitThis": true,
		"strictNullChecks": true,
		"baseUrl": "./",
		"paths": {
			"/*": ["./"]
		}
	},
	"include": [
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		"src/components/account/register/Step1.tsx",
		"pages/account/resetPassword.tsx",
		"src/components/theme/theme.tsx",
		"src/menu/CreateMenu.tsx",
		"pages/_app.sx",
		"pages/api/checkout_sessions/SetStripeKeysHelper.js",
		"src/components/ErrorBoundary.js"
	],
	"exclude": ["node_modules"]
}
